{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@playwright/mcp@latest"],
      "env": {}
    },
    "markitdown": {
      "type": "stdio",
      "command": "uvx",
      "args": ["markitdown-mcp"],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {}
    },
    "youtube": {
      "type": "stdio",
      "command": "npx",
      "args": ["@anaisbetts/mcp-youtube"],
      "env": {}
    },
    "voicevox": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@t09tanaka/mcp-simple-voicevox"],
      "env": {}
    }
  }
}
