{
  "workbench.colorTheme": "Solarized Dark",
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.surround": true,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-e>": false,
    "<C-f>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "<CR>"],
      "after": ["<leader>", "<leader>"]
    },
    {
      "before": ["<leader>", "w"],
      "after": [],
      "commands": [
        {
          "command": ":w"
        }
      ]
    }
  ],
  "editor.rulers": [120],
  "editor.scrollBeyondLastLine": false,
  "files.insertFinalNewline": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "jinja-html": "html"
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "workbench.editorAssociations": {
    "*.wasm": "default",
    "*.tgz": "default"
  },
  "editor.maxTokenizationLineLength": 50000,
  "php.validate.executablePath": "",
  "editor.formatOnSaveMode": "modifications",
  "css.validate": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.inlineSuggest.enabled": true,
  "git.autofetch": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "prettier.documentSelectors": ["**/*.astro"],
  "gitlens.views.remotes.branches.layout": "list",
  "emmet.variables": {
    "lang": "en"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.fontSize": "12",
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.statusBarMessageEnabled": true,
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.messageMaxChars": 50,
  "errorLens.fontStyleItalic": true,
  "errorLens.messageBackgroundMode": "none",
  "[html]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[solidity]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[jsonl]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[ruby]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.tabSize": 2
  },
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.formatAll.terraform": "explicit"
    }
  },
  "[swift]": {
    "editor.defaultFormatter": "vknabel.vscode-apple-swift-format",
    "editor.formatOnSave": false
  },
  "[sql]": {
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
  },
  "[jinja-html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.formatOnSave": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[liquid]": {
    "editor.defaultFormatter": "Shopify.theme-check-vscode"
  },
  "Prettier-SQL.tabSizeOverride": 2,
  "window.zoomLevel": -1,
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    },
    "fish": {
      "path": "fish",
      "args": ["-l"]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "search.useGlobalIgnoreFiles": true,
  "workbench.editor.empty.hint": "hidden",
  "redhat.telemetry.enabled": true,
  "cursor.cpp.disabledLanguages": ["scminput"],
  "jest.runMode": {
    "type": "on-demand"
  },
  "makefile.configureOnOpen": true,
  "prisma.showPrismaDataPlatformNotification": false,
  "markdown.marp.themes": ["./base.css"],
  "markdown.marp.html": "default",
  "powermode.enabled": false,
  "powermode.presets": "simple-rift",
  "roo-cline.allowedCommands": [
    "npm test",
    "npm install",
    "tsc",
    "git log",
    "git diff",
    "git show",
    "pnpm test",
    "pnpm add",
    "npm run build",
    "git add",
    "git commit"
  ],
  "lldb.library": "/Library/Developer/CommandLineTools/Library/PrivateFrameworks/LLDB.framework/Versions/A/LLDB",
  "lldb.launch.expressions": "native",
  "files.associations": {
    "*.html": "jinja-html",
    "*.jinja": "jinja-html",
    "*.jinja2": "jinja-html"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "gitlens.views.scm.grouped.views": {
    "commits": true,
    "branches": true,
    "remotes": true,
    "stashes": true,
    "tags": true,
    "worktrees": true,
    "contributors": true,
    "repositories": false,
    "searchAndCompare": true,
    "launchpad": false
  },
  "liveServer.settings.CustomBrowser": "chrome",
  "calva.keybindingsEnabled": true
}
