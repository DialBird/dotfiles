{
  "workbench.colorTheme": "Solarized Dark",
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.surround": true,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-e>": false,
    "<C-f>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "<CR>"],
      "after": ["<leader>", "<leader>"]
    },
    {
      "before": ["<leader>", "w"],
      "after": [],
      "commands": [
        {
          "command": ":w"
        }
      ]
    }
  ],
  "editor.rulers": [120],
  "editor.scrollBeyondLastLine": false,
  "files.insertFinalNewline": true,
  "workbench.editor.enablePreview": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "[html]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ts]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[solidity]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[ruby]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust",
    "editor.tabSize": 2
  },
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.includeLanguages": {
    "plaintext": "vue"
  },
  "workbench.editor.untitled.hint": "hidden",
  "editor.formatOnSave": true,
  // save時にeslint（prettire含む）でフォーマットする
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  // .（ドット）とかを打つだけで、Enterを押した時と同様に、違うサジェスチョンがOKされてしまうのを防ぐ
  "editor.acceptSuggestionOnCommitCharacter": false,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "search.useGlobalIgnoreFiles": true,
  "workbench.editorAssociations": {
    "*.wasm": "default",
    "*.tgz": "default"
  },
  "ruby.rubocop.onSave": false,
  "editor.maxTokenizationLineLength": 50000,
  "rust-analyzer.checkOnSave.command": "clippy",
  "prettier.eslintIntegration": true,
  "vite.autoStart": false,
  "php.validate.executablePath": "",
  "editor.formatOnSaveMode": "modifications",
  "volar.codeLens.references": false,
  "css.validate": false,
  "chatgpt.method": "GPT3 OpenAI API Key",
  "chatgpt.gpt3.apiKey": "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",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "remote.SSH.remotePlatform": {
    "sandbox.0vg29w.csb": "linux"
  },
  "editor.inlineSuggest.enabled": true,
  "git.autofetch": true,
  "githubPullRequests.pullBranch": "never",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.fontSize": "12",
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.statusBarMessageEnabled": true,
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.messageMaxChars": 50,
  "errorLens.fontStyleItalic": true,
  "errorLens.messageBackgroundMode": "none",
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true
  },
  "window.zoomLevel": -1,
  "prettier.documentSelectors": ["**/*.astro"],
  "errorLens.excludeBySource": ["ts(18048)"],
  "svelte.enable-ts-plugin": true,
  "workbench.welcomePage.hiddenCategories": ["Setup", "Beginner"],
  "gitlens.views.remotes.branches.layout": "list",
  "liveServer.settings.CustomBrowser": "chrome",
  "liveServer.settings.donotShowInfoMsg": true,
  "emmet.variables": {
    "lang": "en"
  },
  "tabnine.experimentalAutoImports": true,
  "security.workspace.trust.untrustedFiles": "open"
}
